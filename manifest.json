{
    "manifest_version": 2,
    "name": "Demo",
    "version": "1.0",
    "description": "Calculate your working hours",
    "icons": {
        "128": "img/icons128.png",
        "48": "img/icons48.png",
        "16": "img/icons16.png"
    },
    "browser_action": {
        "default_icon": "img/icons16.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}